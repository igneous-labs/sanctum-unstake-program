{
  "name": "@unstake-it/sol",
  "version": "0.1.5",
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/igneous-labs/unstake",
    "directory": "ts"
  },
  "homepage": "https://github.com/igneous-labs/unstake/tree/master/ts",
  "license": "MIT",
  "keywords": [],
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -f *.tgz",
    "rebuild": "cd ../ && yarn build && cd ts && yarn build",
    "prebuild": "readlink -f ../target/idl/*.json | xargs -I % cp % src/idl/idl.json && readlink -f ../target/types/*.ts | xargs -I % cp % src/idl/idl.ts",
    "build:cjs": "tsc -p tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "postbuild": "echo '{\"type\":\"commonjs\"}' | npx json > dist/cjs/package.json && echo '{\"type\":\"module\"} ' | npx json > dist/esm/package.json",
    "prepack": "yarn clean && yarn build"
  },
  "dependencies": {
    "@project-serum/anchor": "0.24.2",
    "@soceanfi/solana-stake-sdk": "^0.1.1"
  },
  "devDependencies": {
    "@solana/spl-token": "^0.2.0",
    "@types/bn.js": "^5.1.0",
    "@types/node-fetch": "^2.6.2",
    "bn.js": "^5.2.1",
    "superstruct": "^0.14.2",
    "tsc-alias": "^1.7.1",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@solana/spl-token": "^0.2.0"
  },
  "resolutions": {
    "@solana/web3.js": "1.43.5"
  }
}
